{
  "name": "serious-game-backend",
  "version": "1.0.0",
  "description": "Express Backend for serious-game",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/modern-knowledge/bachelorarbeit/serious-game-backend"
  },
  "author": "Modern Knowledge",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run pre-build && npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "build-docs": "npm run delete-docs && typedoc",
    "pre-build": "rm -rf dist/ && rm -rf src/lib && cp -r ../serious-game-library/src/. ./src/lib",
    "delete-docs": "rm -rf docs/"
  },
  "dependencies": {
    "async": "^3.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "express-winston": "^3.2.1",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rotating-file-stream": "^1.4.2",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.1",
    "uuid": "^3.3.3",
    "validator": "latest",
    "winston": "^2.4.4",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/connect-mongo": "^0.0.42",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-flash": "0.0.0",
    "@types/express-session": "^1.15.13",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.29",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.134",
    "@types/lusca": "^1.6.0",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.0.10",
    "@types/nodemailer": "^6.2.0",
    "@types/passport": "^1.0.0",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.1",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.7",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^3.0.1",
    "@types/uuid": "^3.4.5",
    "@types/validator": "^10.11.3",
    "@types/winston": "^2.3.9",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.2"
  }
}
